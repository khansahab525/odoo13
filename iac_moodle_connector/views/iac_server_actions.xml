<odoo>
    <data>
        <!--&&&&&&&&&&&&&&&&&&&&&& CAMPUS SERVER ACTION &&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&&-->
        <record id="iac_campus_server_action" model="ir.actions.server">
            <field name="name">Sync To Moodle</field>
            <field name="model_id" ref="odoocms.model_odoocms_campus"/>
            <field name="binding_model_id" ref="odoocms.model_odoocms_campus"/>
            <field name="state">code</field>
            <field name="code">action = records.sync_campus_to_moodle()</field>
